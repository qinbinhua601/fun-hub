{
  "name": "fun-hub",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "node ./node_modules/cross-env/dist/bin/cross-env.js PORT=5000 NODE_ENV=dev node ./bin/www",
    "dev2": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "start": "node ./node_modules/cross-env/dist/bin/cross-env.js PORT=80 NODE_ENV=production node ./bin/www",
    "mongod": "mongod --dbpath=./data",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cheerio": "^0.22.0",
    "cookie-parser": "~1.4.3",
    "cross-env": "^4.0.0",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "mongoose": "^4.9.4",
    "morgan": "~1.8.1",
    "serve-favicon": "~2.4.2",
    "superagent": "^3.5.2",
    "vue": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "qs": "^6.4.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^5.0.1",
    "vue-infinite-loading": "^2.0.4",
    "vue-loader": "^11.3.4",
    "vue-router": "^2.5.2",
    "vue-template-compiler": "^2.3.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
